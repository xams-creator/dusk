{
    "name": "@xams-framework/dusk",
    "version": "0.0.9",
    "description": "",
    "keywords": [],
    "license": "ISC",
    "files": [
        "lib",
        "axios.js",
        "history.js",
        "redux.js",
        "router.js"
    ],
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "private": false,
    "peerDependencies": {
        "react": ">=16.9.0",
        "react-dom": ">=16.9.0"
    },
    "dependencies": {
        "@types/react-redux": "latest",
        "@types/react-router-config": "latest",
        "@types/react-router-dom": "latest",
        "axios": "latest",
        "events": "latest",
        "history": "4.9.0",
        "hoist-non-react-statics": "latest",
        "invariant": "latest",
        "react-redux": "latest",
        "react-router-config": "latest",
        "react-router-dom": "latest",
        "redux": "latest",
        "redux-logger": "latest",
        "redux-thunk": "latest",
        "reflect-metadata": "latest"
    },
    "devDependencies": {
        "@types/node": "latest",
        "@types/react": "^17.0.1",
        "@types/react-dom": "^17.0.1",
        "@types/redux-logger": "latest",
        "prettier": "latest",
        "react-test-renderer": "^17.0.1",
        "redux-devtools-extension": "latest",
        "redux-mock-store": "^1.5.3",
        "ts-loader": "latest",
        "typescript": "latest"
    },
    "tempDependencies": {
        "gulp": "^4.0.2",
        "gulp-watch": "^5.0.1",
        "gulp-typescript": "^6.0.0-alpha.1",
        "gulp-babel": "^8.0.0",
        "gulp-concat": "^2.6.1",
        "gulp-strip-code": "^0.1.4",
        "@babel/cli": "^7.13.14",
        "@babel/core": "^7.13.14",
        "@babel/preset-env": "^7.13.12",
        "@babel/preset-react": "^7.13.13",
        "@babel/preset-typescript": "^7.13.0",
        "@babel/plugin-proposal-class-properties": "^7.13.0",
        "@babel/plugin-proposal-decorators": "^7.13.5",
        "@types/redux-actions": "^2.6.1"
    },
    "babel": {
        "plugins": [
            [
                "@babel/plugin-proposal-decorators",
                {
                    "legacy": false,
                    "decoratorsBeforeExport": false
                }
            ],
            [
                "@babel/plugin-proposal-class-properties",
                {
                    "loose": true
                }
            ]
        ],
        "presets": [
            [
                "@babel/preset-env",
                {
                    "useBuiltIns": "entry",
                    "corejs": 3,
                    "exclude": [
                        "transform-typeof-symbol"
                    ]
                }
            ],
            [
                "@babel/preset-react",
                {
                    "development": true,
                    "runtime": "automatic"
                }
            ],
            "@babel/typescript"
        ]
    },
    "scripts": {
        "test1": "babel src --out-dir lib --extensions \".ts,.tsx\"",
        "build1": "tsc && babel src --out-dir lib --extensions \".ts,.tsx\"",
        "watch": "tsc && babel src --watch --out-dir lib --extensions \".ts,.tsx,.sass\"",
        "gulp:watch": "gulp watch",
        "gulp:watch:files": "gulp watch:files",
        "tsc": "tsc",
        "tsc:watchs": "tsc -watch"
    },
    "homepage": "https://github.com/xams-creator/xams-framework-frontend/tree/master/packages/dusk",
    "author": "xams-creator"
}
